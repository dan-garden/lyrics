{
  "name": "get-lyrics",
  "version": "1.0.0",
  "description": "Lyrics API and interface",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "heroku local web",
    "docs": "apidoc -i . -o public/docs/ -e node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dan-garden/lyrics.git"
  },
  "author": "Daniel Garden",
  "license": "MIT",
  "dependencies": {
    "apicache": "^1.5.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsdom": "^15.2.1",
    "node-fetch": "^2.6.0"
  },
  "engines": {
    "node": "10.16.0"
  },
  "apidoc": {
    "name": "Get Lyrics",
    "version": "1.0.0",
    "description": "Song lyrics from different providers using our API",
