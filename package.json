      "Lyrics",
      "Find",
      "SearchGenius",
      "SearchLyricsmode",
      "SearchSonglyrics",
      "SearchAll",
      "GetLyrics",
      "FindGenius",
      "FindLyricsmode",
      "FindSonglyrics",
      "FindAll"
